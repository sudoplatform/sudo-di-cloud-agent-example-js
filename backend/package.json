{
  "name": "@sudoplatform/sudo-di-cloud-agent-example-js",
  "version": "1.0.0",
  "description": "Example app to show how to integrate with the Sudo DI Cloud Agent SDK",
  "author": "Anonyome Labs, Inc.",
  "license": "Apache-2.0",
  "type": "module",
  "dependencies": {
    "@sudoplatform-labs/sudo-di-cloud-agent": "^0.12.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^20.11.16",
    "@types/node-fetch": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  },
  "scripts": {
    "build": "yarn tsc",
    "start": "cd build/src && node server.js",
